      {
        "type": "line",
        "paint": {
          "line-color": "#ffffff",
          "line-width": 3
        }
      }
    ]
  },

  {
    "name": "bijb_kertajati.geojson",
    "displayName": "Kertajati International Airport",
    "description": "",
    "category": "Airport",
    "prompt": ["Airport", "BIJB", "Kertajati", "Airport"],
    "layers": [
      {
        "type": "fill",
        "paint": {
          "fill-color": [
            "case",

            ["==", ["get", "tema"], "Area Terbuka"],
            "#C8E6C9",

            ["==", ["get", "tema"], "Pertanian dan Peternakan"],
            "#81C784",

            ["==", ["get", "tema"], "Bangunan Fasum"],
            "#E57373",

            ["==", ["get", "tema"], "Transportasi"],
            "#64B5F6",

            ["==", ["get", "tema"], "Perairan"],
            "#4FC3F7",

            "#000000"
          ],
          "fill-opacity": 0.65
        }
      }
    ]
  },
  {
    "name": "bandung_lrt.geojson",
    "displayName": "Bandung LRT System",
    "description": "",
    "category": "Rail",
    "prompt": ["Bandung LRT", "LRT Bandung Raya"],
    "layers": [
      {
        "type": "line",
        "layout": {
          "line-join": "round",
          "line-cap": "round"
        },
        "paint": {
          "line-color": "#1f2937",
          "line-width": [
            "interpolate",
            ["linear"],
            ["zoom"],
            10,
            4,
            14,
            8,
            18,
            14
          ]
        }
      },
      {
        "type": "line",
        "layout": {
          "line-join": "round",
          "line-cap": "round"
        },
        "paint": {
          "line-color": "#22c55e",
          "line-width": [
            "interpolate",
            ["linear"],
            ["zoom"],
            10,
            2,
            14,
            5,
            18,
            10
          ]
        }
      },
      {
        "type": "line",
        "layout": {
          "line-join": "round",
          "line-cap": "round"
        },
        "paint": {
          "line-color": "#22c55e",
          "line-width": [
            "interpolate",
            ["linear"],
            ["zoom"],
            10,
            6,
            14,
            12,
            18,
            20
          ],
          "line-opacity": 0.2
        }
      }
    ]
  },
  {
    "name": "tj.geojson",
    "displayName": "Transjakarta",
    "description": "",
    "category": "Transport",
    "prompt": ["Transjarta"],
    "layers": [
      {
        "type": "line",
        "filter": [
          "in",
          ["geometry-type"],
          ["literal", ["LineString", "MultiLineString"]]
        ],
        "layout": {
          "line-join": "round",
          "line-cap": "round"
        },
        "paint": {
          "line-color": "#2563eb",
          "line-width": [
            "interpolate",
            ["linear"],
            ["zoom"],
            10,
            2,
            14,
            4,
            18,
            8
          ]
        }
      },
      {
        "type": "circle",
        "filter": ["==", ["geometry-type"], "Point"],
        "paint": {
          "circle-radius": [
            "interpolate",
            ["linear"],
            ["zoom"],
            10,
            3,
            14,
            6,
            18,
            10
          ],
          "circle-color": "#ffffff",
          "circle-stroke-color": "#2563eb",
          "circle-stroke-width": 2
        }
      }
    ]
  }
]
