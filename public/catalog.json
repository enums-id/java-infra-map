[
  {
    "name": "kcic.geojson",
    "displayName": "High Speed Rail Jakarta-Bandung",
    "description": "",
    "category": "Rail",
    "prompt": ["whoosh", "kcic", "kereta cepat indonesia china"],
    "layers": [
      {
        "type": "line",
        "paint": {
          "line-color": "#000000",
          "line-width": 5
